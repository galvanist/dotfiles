Host bitbucket.org
  User git
  AddKeysToAgent yes
  UseKeychain yes
  IdentityFile ~/.ssh/bitbucket

Host gitlab.com
  Preferredauthentications publickey
  User git
  AddKeysToAgent yes
  UseKeychain yes
  IdentityFile ~/.ssh/gitlab

Host github.com
  AddKeysToAgent yes
  UseKeychain yes
  IdentityFile ~/.ssh/github
